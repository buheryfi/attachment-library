  <ul class="boxlist">
    {{{imageList}}}
  </ul>
</div>
{{{pager}}}
<button class="hidden btn btn-large" id="add_images">{{t "ticket.add"}}</button></br>
<button class="hidden btn btn-large" id="preview_item">{{t "ticket.preview"}}</button></br>
